{
    "ignore": [
        "*",
        "node_modules",
        "coverage",
        "*.pdf",
        "misc",
        "dist",
        ".vscode",
        ".angular"
    ],
    "license": "MIT.txt",
    "licenseFormats": {
        "html|xml|svg": {
            "prepend": "<!--",
            "append": "-->"
        },
        "js|ts|css|scss|less": {
            "prepend": "/*",
            "append": "*/"
        }
    },
    "trailingWhitespace": "TRIM"
}